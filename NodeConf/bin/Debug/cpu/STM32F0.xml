    <serial name="RX" chanel="1" config="0" />
  </item>
  <item name="PA11" pin="21">
    <pwm timer="1"   channel="4" af="2"/>
    <serial name="DE" chanel="1" config="0" />
  </item>
  <item name="PA12" pin="22"></item>
  <item name="PA13" pin="23" default="SWDAT">
    <system name="SWDAT" af="0" />
  </item>
  <item name="PA14" pin="24" default="SWCLK">
    <system name="SWCLK" af="0" />
